<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="20.0" stylesheets="@ExerciseViewer.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.saring.exerciseviewer.gui.EVController">
    <children>
        <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" prefWidth="720.0" styleClass="tabpane-bordered" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab fx:id="tabMain" text="%pv.view.main">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="tabOptional" text="%pv.view.optional">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="tabLaps" text="%pv.view.laps">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="tabSamples" text="%pv.view.samples">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="tabDiagram" text="%pv.view.diagram">
                    <content>
                    </content>
                </Tab>
                <Tab fx:id="tabTrack" text="%pv.view.track">
                    <content>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox alignment="CENTER_RIGHT">
            <children>
                <Button cancelButton="true" onAction="#onClose" text="%pv.view.close.Action.text">
                    <padding>
                        <Insets bottom="6.0" left="16.0" right="16.0" top="6.0"/>
                    </padding>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
    </padding>
</VBox>
